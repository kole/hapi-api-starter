{
    "moduleDirectories": ["node_modules"],
    "transform": {
      "^.+\\.js$": "babel-jest"
    },
    "globals": {
        "ENV": "test"
    },
    "collectCoverage": true,
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    },
    "verbose": true
}
